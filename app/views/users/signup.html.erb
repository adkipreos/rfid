<% provide(:title, 'Sign up') %>
<h1>Create</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :lastname %>
      <%= f.text_field :lastname %>

      <%= f.label :rfid %>
      <%= f.text_field :rfid %>


      <%= f.submit "Create user", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
